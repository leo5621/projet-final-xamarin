<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjetNote.FicheRdv"
             Appearing="ContentPage_Appearing">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Fiche de rendez-vous" FontSize="Large"/>
            <Label Text="Date" x:Name="Lb_heure"/>
            <Label Text="Libelle" x:Name="Lb_Libelle"/>
            <Label Text="Durée" x:Name="Lb_duree"/>
            <Label Text="Urgence" x:Name="Lb_urgence"/>
            <Label Text="Nom du patient" x:Name="Lb_Patient"/>
            <Button Text="Accepter" x:Name="Bt_valider"/>
            <Label Text="" x:Name="Lb_patientPris"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>